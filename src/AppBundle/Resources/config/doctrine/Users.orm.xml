<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="users">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastName" type="string" column="last_name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hasShared" type="boolean" column="has_shared" nullable="false"/>
    <field name="sharedOn" type="string" column="shared_on" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="confirmed" type="boolean" column="confirmed" nullable="false"/>
    <field name="confirmationToken" type="string" column="confirmation_token" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="points" type="integer" column="points" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="publicKey" type="string" column="public_key" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userLevel" type="string" column="user_level" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
